CREATE VIRTUAL TABLE IF NOT EXISTS feed_source USING FTS5(
    link TEXT NOT NULL PRIMARY KEY,
    tokenize="trigram"
);

save:
INSERT OR REPLACE INTO feed_source(link) VALUES (:link);

countSearchResults:
SELECT COUNT(*) FROM feed_source WHERE feed_source MATCH :searchQuery;

search:
SELECT * FROM feed_source
WHERE feed_source MATCH :searchQuery
LIMIT :limit OFFSET :offset;
